{
  "buildCommand": "npm run vercel-build",
  "framework": "nextjs",
  "nodeVersion": "20.x",
  "installCommand": "npm ci",
  "github": {
    "silent": true,
    "deploymentEnabled": true,
    "autoJobCancelation": true
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": false
    }
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "crons": []
}
